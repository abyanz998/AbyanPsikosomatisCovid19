            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="30dp"
            android:textColor="#448985"
            android:textStyle="bold"
            android:text="7. Mudah marah"/>
